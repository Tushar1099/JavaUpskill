package basicDS;

public class ArmStrongNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 154;
		int temp = num;
		int cube = 0;
		int armNum = 0;
		while(num!=0) {
			int temp1 = num%10;
			cube = temp1 * temp1 * temp1;
			armNum = armNum + cube;
			num = num/10;
		}
		if(temp == armNum) {
			System.out.println("ArmStrong Number");
		}
		else {
			System.out.println("Not Arm");
		}
	}

}
