package basicDS;

public class ArmStrongNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 153;
		int armNum = 0;
		while(num!=0) {
			int temp = num%10;
			armNum = temp * temp * temp;
//			 = +temp;
			num = num/10;
			System.out.println(armNum);
		}
		
	}

}
