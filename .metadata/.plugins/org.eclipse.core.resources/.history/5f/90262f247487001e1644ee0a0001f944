package basicDS;

public class ArmStrongNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 153;
		int temp = num;
		int cube = 0;
		int armNum = 0;
		int count = 0;
		while(num!=0) {
			int temp1 = num%10;
			count++;
			num = num/10;
		}
		System.out.println(count);
		while(num!=0) {
			int temp1 = num%10;
			while(count!=0) {
				cube = cube * temp1;
			}
			armNum = armNum + cube;
			num = num/10;
			System.out.println(cube);
		}
		
		if(temp == armNum) {
			System.out.println("ArmStrong Number");
		}
		else {
			System.out.println("Not Arm");
		}
	}

}
